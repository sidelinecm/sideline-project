#
#  Strict cache control + GZIP
#

# Critical Font
/fonts/*.woff2
  Cache-Control: public, max-age=31536000, immutable

# Image lazyload
/images/*
  Cache-Control: public, max-age=31536000, immutable

# CSS/JS Assets
/dist/output.css
  Cache-Control: public, max-age=31536000, immutable
/main.js
  Cache-Control: public, max-age=31536000, immutable

# HTML no-cache
/index.html
  Cache-Control: no-cache
/about.html
  Cache-Control: no-cache
/profiles.html
  Cache-Control: no-cache
/locations.html
  Cache-Control: no-cache
/faq.html
  Cache-control: no-cache
/blog.html
  Cache-control: no-cache
